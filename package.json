{
  "name": "sup-flang-plugin",
  "version": "0.2.1",
  "description": "Easy localization for Superpowers, the extensible HTML5 2D+3D game engine.",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-flang-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "dependencies": {},
  "devDependencies": {
    "typedoc": "0.2.x",
    "typescript": "1.4.x"
  },
  "keywords": [
    "superpowers",
    "plugin",
    "language",
    "lang",
    "i18n",
    "internationalization"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-flang-plugin/issues"
  },
  "scripts": {
    "compilets": "tsc api/fLang.ts --out gitignore/fLang.js --module commonjs --target ES5",
    "gendoc": "typedoc --out public/docs api/fLang.ts --mode file --target ES5 --name \"fLang plugin\" --readme tutorial.md --verbose --theme minimal",
    
    "linksrcdev": "mklink /j \"../../app/plugins/florentpoujol/fLang\" \"./\"",
    "linksrcrelease": "mklink /j \"../../../release/app/plugins/florentpoujol/fLang\" \"./\"",

    "linkprojdev": "mklink /j \"../../data/projects/fLang\" \"./project/fLang\"",
    "linkprojrelease": "mklink /j \"../../../localprojects/fLang\" \"./project/fLang\""
  },
  "maintainers": [
    "Florent Poujol <florent.poujol@gmail.com>"
  ],
  "homepage": "https://github.com/florentpoujol/superpowers-flang-plugin",
  "directories": {
    "doc": "docs"
  },
  "license": "MIT"
}
